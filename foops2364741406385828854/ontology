    <!-- https://w3id.org/okn/o/sd#adjustsVariable -->


    <owl:ObjectProperty rdf:about="https://w3id.org/okn/o/sd#adjustsVariable">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/okn/o/sd#Parameter"/>
        <rdfs:range rdf:resource="https://w3id.org/okn/o/sd#Variable"/>
        <rdfs:comment xml:lang="en">Property that links parameter with the variable they adjust. This property can be used when parameters quantify variables without directly representing them. For example, a &quot;fertilizer percentage adjustment&quot; parameter can quantify a &quot;fertilizer price&quot; variable</rdfs:comment>
        <rdfs:label xml:lang="en">adjusts variable</rdfs:label>
    </owl:ObjectProperty>
    


