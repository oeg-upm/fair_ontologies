	dcterms:title "Ontology/Vocabulary minimum metadata is declared"@en ;
	rdfs:label "OM1 test: Ontology/Vocabulary minimum metadata is declared"^^xsd:string ;
	vivo:abbreviation "OM1-T"^^xsd:string ;
